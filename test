# Starting the stable-diffusion-webui with pinggy

from multiprocessing import Process
import sys
import time

# åˆ›å»ºä¸€ä¸ªç©ºçš„æ—¥å¿—æ–‡ä»¶
!touch log.txt
open('log.txt', 'w').close()

def run_app():
    # å¯åŠ¨ stable-diffusion-webui å¹¶å°†è¾“å‡ºé‡å®šå‘åˆ° log.txt
    cmd = f"python /kaggle/working/stable-diffusion-webui/webui.py --port 8188 & ssh -o StrictHostKeyChecking=no -p 80 -R0:localhost:8188 a.pinggy.io > log.txt"
    get_ipython().system(cmd)

def print_url():
    print("waiting for output")
    time.sleep(2)  # ç­‰å¾…è¾“å‡ºå‡ºç°åœ¨æ—¥å¿—æ–‡ä»¶ä¸­
    sys.stdout.flush()
    
    found = False
    with open('log.txt', 'r') as file:
        end_word = '.pinggy.link'
        for line in file:
            start_index = line.find("http:")
            if start_index != -1:
                end_index = line.find(end_word, start_index)
                if end_index != -1:
                    print("ğŸ˜ ğŸ˜ ğŸ˜")
                    print("URL: " + line[start_index:end_index + len(end_word)])
                    print("ğŸ˜ ğŸ˜ ğŸ˜")
                    found = True
    if not found:
        print_url()
    else:
        with open('log.txt', 'r') as file:
            for line in file:
                print(line)

# å¯åŠ¨åº”ç”¨ç¨‹åºå’Œ URL æ‰“å°è¿›ç¨‹
p_app = Process(target=run_app)
p_url = Process(target=print_url)
p_app.start()
p_url.start()
p_app.join()
p_url.join()

